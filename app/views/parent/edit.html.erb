<h1>Parent Update Panel</h1>

<%= form_for @parent, :method => :put do |f| %>
  First Name: <%= f.text_field :firstname %><br />
  Last Name: <%= f.text_field :lastname %><br />
  Phone: <%= f.telephone_field :phone %><br />
  Email: <%= f.email_field :email %><br />
  <%= f.submit "Update"%>
<% end %>
<%= link_to "Back", parent_path %>