<h1>Group Information</h1>

<h2>Your Group Information</h2>
<table>
  <tr>
    <th>ID</th>
    <th>Title</th>
    <th>Mentor</th>
    <th>Members</th>
    
  </tr>
 <% @groups.each do |group| %>
  <tr>
    <td><%= group.id %></td>
    <td><%= group.title %></td>
    <td><%= group.mentor.name %></td>
    <td><%= group.members %></td>

  </tr>
  <% end %>
</table>
