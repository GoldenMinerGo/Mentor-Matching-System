<div class='row'>
  <div class="col-xs-12 col-sm-12 col-md-12 col-lg-12">
    <h2>Group Information</h2>
    <div class="table-responsive">
      <table class='table'>
        <thead>
          <tr>
            <th>Members</th>
            <th>Competition</th>
            <th>Description</th>
          </tr>
        </thead>
        <tbody>
          <tr>
            <td><%= @group.members %></td>
            <div class='time-board time-board-hover' value = '<%=@time_slot%>' ></div>
            <td><%= @group.competitions %></td>
            <td><%= @group.description  %></td>
          </tr>
        </tbody>
      </table>
    </div>
  </div>
</div>
