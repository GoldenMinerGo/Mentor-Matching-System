<h1>Weclome to the Mentor Matching System Version 1.0</h1>

<% @page_title = "UserAuth | Login" -%>
<div class= "Sign_Form">
  <h1>Log in</h1>
  <%= form_tag(:controller => 'welcome', :action => 'login_attempt') do %>
    <p>Username:</br> <%= text_field_tag(:username) %></p> 
    <p>Password:</br> <%= password_field_tag :login_password %></p> 
    <%= submit_tag("Log In") %>
  <% end %>
</div>

<%= link_to "Forget Password", welcome_forget_password_path %>
<%= link_to "Sign Up", users_new_path %>